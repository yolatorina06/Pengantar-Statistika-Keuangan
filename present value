fungsipv <- function(num, A, i, t, m = TRUE )
  switch(num, 
         satu = {
           bungatunggal = A/(1+i*t)
           print(bungatunggal)
         },
         dua = {
           bungamajemuknominal = A/(1+(i/m))^(m*t)
           print(bungamajemuknominal)
         },
         tiga = {
           bungamajemukkontinu = A/e^(i*t)
           print(bungamajemukkontinu)
         }
  )
